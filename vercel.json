{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/sitemap2.xml",
      "destination": "/sitemap2.xml"
    },
    {
      "source": "/sitemap3.xml",
      "destination": "/sitemap3.xml"
    },
    {
      "source": "/atom.xml",
      "destination": "/atom.xml"
    },
    {
      "source": "/atom1.xml",
      "destination": "/atom1.xml"
    }
  ],
  "headers": [
    {
      "source": "/(sitemap.*\\.xml|atom.*\\.xml)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400"
        }
      ]
    }
  ]
}
